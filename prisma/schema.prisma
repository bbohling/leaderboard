// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id         Int    @id @default(autoincrement())
  user       String
  score      Int
  wave       Int
  damage     Int
  difficulty String
  kills      Int
  version    String
}
